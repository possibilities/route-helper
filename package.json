    "babel-cli": "6.4.0",
    "babel-core": "6.4.0",
    "babel-eslint": "4.1.6",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "chai": "3.4.1",
    "eslint": "1.10.3",
    "mocha": "2.3.4",
    "pre-commit": "1.1.2",
    "rimraf": "2.5.0"
  }
